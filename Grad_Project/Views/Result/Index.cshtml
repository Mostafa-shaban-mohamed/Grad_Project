@model IEnumerable<Grad_Project.Models.Result_tbl>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Total_Mark)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Achieved_Mark)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Course_tbl.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student_tbl.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Total_Mark)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Achieved_Mark)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Course_tbl.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Student_tbl.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
